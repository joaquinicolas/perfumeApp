{
   "type": "sqlite",
   "database": "db.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "electron_app/entity/*.ts",
      "electron_app/entity/dist/*.js"
   ],
   "migrations": [
      "electron_app/migration/**/*.ts"
   ],
   "subscribers": [
      "electron_app/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "electron_app/entity",
      "migrationsDir": "electron_app/migration",
      "subscribersDir": "electron_app/subscriber"
   }
}
